  <div typeof="skos:Concept" about="http://iflastandards.info/ns/isbd/terms/contentqualification/motion/T1002">
    <div property="skos:prefLabel" xml:lang="it" content="fissa"></div>
    <div property="skos:prefLabel" xml:lang="en" content="Image content that is perceived to be static."></div>
    <div property="skos:definition" xml:lang="en" content="Image content that is perceived to be static."></div>
    <div property="skos:prefLabel" xml:lang="hr" content="nepokretna"></div>
