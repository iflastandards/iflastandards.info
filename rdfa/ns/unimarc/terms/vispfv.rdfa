    <div property="skos:notation" xml:lang="en" content="f"></div>
    <div property="skos:prefLabel" xml:lang="en" content="Quadruplex (reel)"></div>
    <div rel="skos:inScheme" resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"></div>
    <div property="skos:definition" xml:lang="en" content="Video system using four recording heads, developed by Ampex in the 1950s."></div>
  </div>
  <div typeof="skos:Concept" about="http://iflastandards.info/ns/unimarc/terms/vispfv#k">
    <div property="skos:definition" xml:lang="en" content="Digital Versatile Disc."></div>
