    <div rel="skos:inScheme" resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"></div>
  </div>
  <div typeof="skos:Concept" about="http://iflastandards.info/ns/unimarc/terms/vispfv#b">
    <div property="skos:notation" xml:lang="en" content="b"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div rel="reg:identifier" resource="http://metadataregistry.org/concepts/7816"></div>
    <div property="skos:prefLabel" xml:lang="en" content="VHS (videocassette)"></div>
    <div rel="skos:inScheme" resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"></div>
    <div property="skos:definition" xml:lang="en" content="Home video format developed by Japan Victor Corporation (JVC) and marketed in 1977."></div>
  </div>
  <div typeof="skos:Concept" about="http://iflastandards.info/ns/unimarc/terms/vispfv#i">
    <div property="skos:definition" xml:lang="en" content="Home video format developed in Europe by Philips since 1980. Reversible."></div>
    <div property="skos:notation" xml:lang="en" content="i"></div>
    <div rel="reg:identifier" resource="http://metadataregistry.org/concepts/7823"></div>
    <div property="skos:prefLabel" xml:lang="en" content="V2000 (videocassette)"></div>
    <div rel="skos:inScheme" resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"></div>
