    <div property="skos:prefLabel" xml:lang="en" content="not a videorecording"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:notation" xml:lang="en" content="x"></div>
    <div rel="reg:identifier" resource="http://metadataregistry.org/concepts/7827"></div>
  </div>
