  <div typeof="skos:Concept" about="http://iflastandards.info/ns/unimarc/terms/vispfv#d">
    <div rel="skos:inScheme">
      <div typeof="skos:ConceptScheme" about="http://iflastandards.info/ns/unimarc/terms/vispfv#">
        <div property="dc:title" content="UNIMARC: Visual projections, etc.: Presentation format - videorecording"></div>
      </div>
    </div>
    <div property="skos:definition" xml:lang="en" content="The standard ½-inch reel-to-reel helical scan videotape format. Named for the Japanese standards committee (the Electronics Industries Association of Japan) which set the standard for ½ inch video tape recorders manufactured since 1969."></div>
    <div property="skos:prefLabel" xml:lang="en" content="EIAJ (reel)"></div>
    <div rel="reg:status">
      <div typeof="skos:Concept" about="http://metadataregistry.org/uri/RegStatus/1001">
        <div property="skos:prefLabel" xml:lang="en" content="Published"></div>
      </div>
    </div>
    <div property="skos:notation" xml:lang="en" content="d"></div>
    <div rel="reg:identifier" resource="http://metadataregistry.org/concepts/7818"></div>
  </div>
  <div typeof="skos:Concept" about="http://iflastandards.info/ns/unimarc/terms/vispfv#j">
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:notation" xml:lang="en" content="j"></div>
    <div property="skos:prefLabel" xml:lang="en" content="Video8 (videocassette)"></div>
    <div property="skos:definition" xml:lang="en" content="Home video format."></div>
    <div rel="skos:inScheme" resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"></div>
