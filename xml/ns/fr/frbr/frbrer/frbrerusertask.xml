<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:reg="http://metadataregistry.org/uri/profile/regap/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: FRBRer User Tasks -->
    <skos:ConceptScheme rdf:about="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask">
        <dc:title>FRBRer User Tasks</dc:title>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask#T1001"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask#T1002"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask#T1004"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask#T1003"/>
    </skos:ConceptScheme>

<!-- Concept: find  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask#T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1232"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask"/>
      <skos:prefLabel xml:lang="en">find</skos:prefLabel>
      <skos:definition xml:lang="en">To locate either a single entity or a set of entities in a file or database as the result of a search using an attribute or relationship of the entity.</skos:definition>
      <skos:scopeNote xml:lang="en">Includes only entities of interest to users of bibliographic records.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: identify  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask#T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1233"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask"/>
      <skos:prefLabel xml:lang="en">identify</skos:prefLabel>
      <skos:definition xml:lang="en">To confirm that the entity described corresponds to the entity sought, or to distinguish between two or more entities with similar characteristics.</skos:definition>
      <skos:scopeNote xml:lang="en">Includes only entities of interest to users of bibliographic records.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: obtain  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask#T1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1235"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask"/>
      <skos:prefLabel xml:lang="en">obtain</skos:prefLabel>
      <skos:definition xml:lang="en">To acquire an entity through purchase, loan, etc., or to access an entity electronically through an online connection to a remote computer.</skos:definition>
      <skos:scopeNote xml:lang="en">Includes only entities of interest to users of bibliographic records.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: select  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask#T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1234"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbrer/frbrerusertask"/>
      <skos:prefLabel xml:lang="en">select</skos:prefLabel>
      <skos:definition xml:lang="en">To choose an entity that meets the user’s requirements with respect to content, physical format, etc., or to reject an entity as being inappropriate to the user’s needs</skos:definition>
      <skos:scopeNote xml:lang="en">Includes only entities of interest to users of bibliographic records.</skos:scopeNote>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>
