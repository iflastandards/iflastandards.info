      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1243"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/fr/frsad/frsadusertask"/>
      <skos:prefLabel xml:lang="en">explore</skos:prefLabel>
      <skos:definition xml:lang="en">Explore relationships between subjects and. or, their appellations (e.g., to explore relationships in order to understand the structure of a subject domain and its terminology).</skos:definition>
    </skos:Concept>
<!-- Concept: find  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/fr/frsad/frsadusertask#T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/fr/frsad/frsadusertask"/>
