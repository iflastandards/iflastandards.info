      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1240"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/fr/frsad/frsadusertask"/>
      <skos:prefLabel xml:lang="en">find</skos:prefLabel>
      <skos:definition xml:lang="en">Find one or more subjects and, or, their appellations, that correspond(s) to the userâ€™s stated criteria, using attributes and relationships.</skos:definition>
    </skos:Concept>
<!-- Concept: identify  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/fr/frsad/frsadusertask#T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/fr/frsad/frsadusertask"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1241"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/fr/frsad/frsadusertask"/>
      <skos:prefLabel xml:lang="en">identify</skos:prefLabel>
      <skos:definition xml:lang="en">Identify a subject and, or, its appellation based on its attributes or relationships (i.e., to distinguish between two or more subjects or appellations with similar characteristics and to confirm that the appropriate subject or appellation has been found).</skos:definition>
    </skos:Concept>
<!-- Concept: select  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/fr/frsad/frsadusertask#T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/fr/frsad/frsadusertask"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1242"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/fr/frsad/frsadusertask"/>
      <skos:prefLabel xml:lang="en">select</skos:prefLabel>
      <skos:definition xml:lang="en">Select a subject and, or, its appellation appropriate to the user's needs (i.e., to choose or reject based on the user's requirements and needs).</skos:definition>
    </skos:Concept>

