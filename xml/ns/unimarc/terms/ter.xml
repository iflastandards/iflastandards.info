<!-- Scheme: UNIMARC: Type of electronic resource -->
    <skos:ConceptScheme rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#">
        <dc:title>UNIMARC: Type of electronic resource</dc:title>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#e"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#v"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#b"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#f"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#g"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#i"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#a"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#j"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#z"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#c"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#h"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#d"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#u"/>
    </skos:ConceptScheme>

<!-- Concept: bibliographic data  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#e" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6487"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">bibliographic data</skos:prefLabel>
      <skos:notation xml:lang="en">e</skos:notation>
      <skos:definition xml:lang="en">The resource consists of data with bibliographic citations. This includes data from library catalogs or citation databases. The data may be in a structured or unstructured form.</skos:definition>
      <skos:prefLabel xml:lang="pt">dados bibliográficos</skos:prefLabel>
      <skos:prefLabel xml:lang="it">dati bibliografici</skos:prefLabel>
    </skos:Concept>
<!-- Concept: combination  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#v" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6494"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">combination</skos:prefLabel>
      <skos:notation xml:lang="en">v</skos:notation>
      <skos:definition xml:lang="en">A data file with different types of content.</skos:definition>
      <skos:prefLabel xml:lang="pt">combinação</skos:prefLabel>
      <skos:prefLabel xml:lang="it">combinazione</skos:prefLabel>
    </skos:Concept>
<!-- Concept: computer program  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6484"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">computer program</skos:prefLabel>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:definition xml:lang="en">A data file that contains an ordered set of instructions directing the computer to perform basic operations and identifying the information and mechanisms required.</skos:definition>
      <skos:scopeNote xml:lang="en">Includes videogame and microcomputer software and computer models.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">programma informatico</skos:prefLabel>
      <skos:prefLabel xml:lang="pt">programas de computador</skos:prefLabel>
    </skos:Concept>
<!-- Concept: font  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#f" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6488"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">font</skos:prefLabel>
      <skos:notation xml:lang="en">f</skos:notation>
      <skos:definition xml:lang="en">The resource contains information for a computer to produce fonts.</skos:definition>
      <skos:prefLabel xml:lang="pt">fonte</skos:prefLabel>
      <skos:prefLabel xml:lang="it">set di caratteri</skos:prefLabel>
    </skos:Concept>
<!-- Concept: game  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#g" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6489"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">game</skos:prefLabel>
      <skos:notation xml:lang="en">g</skos:notation>
      <skos:definition xml:lang="en">The resource is a game, intended for recreational or educational use. Generally games consist of text and software.</skos:definition>
      <skos:scopeNote xml:lang="en">Includes videogames.</skos:scopeNote>
      <skos:prefLabel xml:lang="pt">jogo</skos:prefLabel>
      <skos:prefLabel xml:lang="it">gioco</skos:prefLabel>
    </skos:Concept>
<!-- Concept: interactive multimedia  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#i" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6491"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">interactive multimedia</skos:prefLabel>
      <skos:notation xml:lang="en">i</skos:notation>
      <skos:definition xml:lang="en">The resource supports navigation through and manipulation of many kinds of media (i.e. audio, video, etc.)</skos:definition>
      <skos:prefLabel xml:lang="pt">multimedia interactivo</skos:prefLabel>
      <skos:prefLabel xml:lang="it">multimediale interattivo</skos:prefLabel>
    </skos:Concept>
<!-- Concept: numeric  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6483"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">numeric</skos:prefLabel>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:definition xml:lang="en">A data file that contains mostly numbers or representation by numbers, such as records containing information on student test scores, information on football team statistics, etc.  The information may be original surveys or information that has been summarized or statistically manipulated.</skos:definition>
      <skos:prefLabel xml:lang="pt">numérico</skos:prefLabel>
      <skos:prefLabel xml:lang="it">dati numerici</skos:prefLabel>
    </skos:Concept>
<!-- Concept: online system or service  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#j" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6492"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">online system or service</skos:prefLabel>
      <skos:notation xml:lang="en">j</skos:notation>
      <skos:definition xml:lang="en">The resource is an online system or service and may contain non-bibliographic information. An online system or service supports system-based user interaction.</skos:definition>
      <skos:scopeNote xml:lang="en">Examples include online library systems; FTP sites; electronic bulletin boards; discussion groups/lists; World Wide Web sites, network information centres.</skos:scopeNote>
      <skos:prefLabel xml:lang="pt">serviço ou sistema em linha</skos:prefLabel>
      <skos:prefLabel xml:lang="it">sistema o servizio on line</skos:prefLabel>
    </skos:Concept>
<!-- Concept: other  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6495"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:prefLabel xml:lang="pt">outro</skos:prefLabel>
      <skos:prefLabel xml:lang="it">altro</skos:prefLabel>
    </skos:Concept>
<!-- Concept: representational  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6485"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">representational</skos:prefLabel>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:definition xml:lang="en">A data file that contains pictorial or graphic information that can be manipulated in conjunction with other types of files to produce graphic patterns that can be used to interpret and give meaning to the information.</skos:definition>
      <skos:prefLabel xml:lang="pt">figurativo</skos:prefLabel>
      <skos:prefLabel xml:lang="it">dati grafici</skos:prefLabel>
    </skos:Concept>
<!-- Concept: sound  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#h" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6490"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">sound</skos:prefLabel>
      <skos:notation xml:lang="en">h</skos:notation>
      <skos:definition xml:lang="en">The resource consists of data encoding sounds produced by the computer.</skos:definition>
      <skos:prefLabel xml:lang="pt">som</skos:prefLabel>
      <skos:prefLabel xml:lang="it">suono</skos:prefLabel>
    </skos:Concept>
<!-- Concept: text  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#d" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6486"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">text</skos:prefLabel>
      <skos:notation xml:lang="en">d</skos:notation>
      <skos:definition xml:lang="en">A data file that contains mostly alphabetic information (words or sentences) converted into a coded format that can be processed, sorted, and manipulated by machine, and then retrieved in many optional formats.</skos:definition>
      <skos:prefLabel xml:lang="pt">texto</skos:prefLabel>
      <skos:prefLabel xml:lang="it">dati testuali</skos:prefLabel>
    </skos:Concept>
<!-- Concept: unknown  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/ter#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6493"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/ter#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:prefLabel xml:lang="pt">desconhecido</skos:prefLabel>
      <skos:prefLabel xml:lang="it">sconosciuto</skos:prefLabel>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>
