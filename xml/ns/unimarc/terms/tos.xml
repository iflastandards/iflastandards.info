<!-- Concept: vocal score, accompaniment reduced for keyboard  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6455"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">vocal score, accompaniment reduced for keyboard</skos:prefLabel>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:definition xml:lang="en">Score where accompaniment is reduced for keyboard instrument(s), and the voice and/or instrumental solo part(s) remain unaltered.</skos:definition>
      <skos:altLabel xml:lang="en">chorus score</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition chant et piano</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition vocale</skos:altLabel>
      <skos:altLabel xml:lang="de">Klavierauszug</skos:altLabel>
      <skos:altLabel xml:lang="de">Sparte</skos:altLabel>
      <skos:altLabel xml:lang="it">Spartito</skos:altLabel>
      <skos:altLabel xml:lang="es">partitura vocal</skos:altLabel>
    </skos:Concept>
<!-- Concept: voice score, chorus score, accompaniment has been dropped  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#d" xml:lang="en">
