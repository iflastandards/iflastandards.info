<!-- Concept: choir-book  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#i" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6461"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">choir-book</skos:prefLabel>
      <skos:notation xml:lang="en">i</skos:notation>
      <skos:definition xml:lang="en">Score format giving the different vocal parts on two facing pages of the book.</skos:definition>
      <skos:altLabel xml:lang="fr">livre de choeur</skos:altLabel>
      <skos:altLabel xml:lang="de">Chorbuch</skos:altLabel>
      <skos:altLabel xml:lang="es">libro de coro</skos:altLabel>
      <skos:altLabel xml:lang="it">libro corale</skos:altLabel>
      <skos:altLabel xml:lang="hu">kóruskönyv</skos:altLabel>
    </skos:Concept>
<!-- Concept: close score  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#g" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6459"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">close score</skos:prefLabel>
      <skos:notation xml:lang="en">g</skos:notation>
      <skos:definition xml:lang="en">Score format giving all the parts on a minimum number of staves, normally two, as with hymnals.</skos:definition>
      <skos:altLabel xml:lang="en">short score</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition réduite</skos:altLabel>
      <skos:altLabel xml:lang="it">partitura ristretta</skos:altLabel>
      <skos:altLabel xml:lang="es">partitura reducida</skos:altLabel>
    </skos:Concept>
<!-- Concept: compressed score  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#j" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6462"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">compressed score</skos:prefLabel>
      <skos:notation xml:lang="en">j</skos:notation>
      <skos:definition xml:lang="en">Score of only voices and continuo, omitting the other instruments.</skos:definition>
      <skos:altLabel xml:lang="fr">particella</skos:altLabel>
      <skos:altLabel xml:lang="de">Cembalo-Partitur</skos:altLabel>
      <skos:altLabel xml:lang="de">Generalbaβpartitur</skos:altLabel>
      <skos:altLabel xml:lang="es">partitura de coro y continuo</skos:altLabel>
      <skos:altLabel xml:lang="it">particella</skos:altLabel>
    </skos:Concept>
<!-- Concept: condensed score with text and chord symbols  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#o" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6465"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">condensed score with text and chord symbols</skos:prefLabel>
      <skos:notation xml:lang="en">o</skos:notation>
      <skos:definition xml:lang="en">Score frequently used in modern pop music, consisting generally in a melodic line with letters and/or graphic symbols representing chords or finger positions on the guitar.</skos:definition>
      <skos:altLabel xml:lang="en">text and chords</skos:altLabel>
      <skos:altLabel xml:lang="fr">texte et accords</skos:altLabel>
      <skos:altLabel xml:lang="it">testo e accordi</skos:altLabel>
    </skos:Concept>
<!-- Concept: condensed score, piano conductor score  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#e" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6457"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">condensed score, piano conductor score</skos:prefLabel>
    </skos:Concept>
<!-- Concept: full score  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6453"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">full score</skos:prefLabel>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:definition xml:lang="en">Score format consisting of a series of staves on which all the instrumental and/or vocal parts of a musical work are written, one under the other in vertical alignment, so that they can be read simultaneously</skos:definition>
      <skos:altLabel xml:lang="en">score</skos:altLabel>
      <skos:altLabel xml:lang="en">performance score</skos:altLabel>
      <skos:altLabel xml:lang="en">playing score</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition d’orchestre</skos:altLabel>
      <skos:altLabel xml:lang="fr">grande partition</skos:altLabel>
      <skos:altLabel xml:lang="de">Partitur</skos:altLabel>
      <skos:altLabel xml:lang="de">Orchesterpartitur</skos:altLabel>
      <skos:altLabel xml:lang="de">Spielpartitur</skos:altLabel>
      <skos:altLabel xml:lang="it">partitura</skos:altLabel>
      <skos:altLabel xml:lang="it">parte con guida</skos:altLabel>
      <skos:altLabel xml:lang="es">Partitura</skos:altLabel>
      <skos:altLabel xml:lang="hu">partitúra</skos:altLabel>
      <skos:altLabel xml:lang="hu">vezérkönyv</skos:altLabel>
      <skos:altLabel xml:lang="ru">партитура</skos:altLabel>
      <skos:altLabel xml:lang="ru">partitura</skos:altLabel>
    </skos:Concept>
<!-- Concept: graphic score  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#f" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6458"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">graphic score</skos:prefLabel>
      <skos:notation xml:lang="en">f</skos:notation>
      <skos:definition xml:lang="en">Score format presenting only the principal musical parts on a minimum number of staves, and generally organized by instrumental sections. A conductor score is a performance part for a particular instrument of an ensemble work to which cues have been added for the other instruments to permit the performer of the part also to conduct the performance.</skos:definition>
      <skos:altLabel xml:lang="en">conductor score</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition condensée</skos:altLabel>
      <skos:prefLabel xml:lang="fr">partition conducteur</skos:prefLabel>
      <skos:altLabel xml:lang="de">Direktion</skos:altLabel>
      <skos:altLabel xml:lang="de">Direktionsstimme</skos:altLabel>
      <skos:altLabel xml:lang="de">Klavierpartitur</skos:altLabel>
      <skos:altLabel xml:lang="it">partitura condensata</skos:altLabel>
      <skos:altLabel xml:lang="it">parte di conduttore</skos:altLabel>
      <skos:altLabel xml:lang="es">partitura guión</skos:altLabel>
      <skos:altLabel xml:lang="es">particela de director</skos:altLabel>
    </skos:Concept>
<!-- Concept: multiple formats  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#m" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
