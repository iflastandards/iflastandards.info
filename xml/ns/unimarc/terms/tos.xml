    </skos:Concept>
<!-- Concept: pseudo-score  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#k" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6463"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">pseudo-score</skos:prefLabel>
      <skos:notation xml:lang="en">k</skos:notation>
      <skos:definition xml:lang="en">Score without vertical coincidence of voices.</skos:definition>
      <skos:altLabel xml:lang="fr">pseudo-partition</skos:altLabel>
      <skos:altLabel xml:lang="de">Pseudo-Partitur</skos:altLabel>
      <skos:altLabel xml:lang="es">Pseudopartitura</skos:altLabel>
      <skos:altLabel xml:lang="it">Pseudopartitura</skos:altLabel>
    </skos:Concept>
<!-- Concept: score (miniature or study size)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
