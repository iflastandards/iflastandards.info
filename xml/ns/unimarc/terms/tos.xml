      <skos:prefLabel xml:lang="en">score (miniature or study size)</skos:prefLabel>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:definition xml:lang="en">A musical score not primarily intended for performance use, with the notation and/or text reduced in size.</skos:definition>
      <skos:altLabel xml:lang="en">pocket score</skos:altLabel>
      <skos:altLabel xml:lang="en">study score</skos:altLabel>
      <skos:altLabel xml:lang="en">miniature score</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition de travail</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition de poche</skos:altLabel>
      <skos:altLabel xml:lang="fr">partition miniature</skos:altLabel>
      <skos:altLabel xml:lang="fr">petite partition</skos:altLabel>
      <skos:altLabel xml:lang="de">Studienpartitur</skos:altLabel>
      <skos:altLabel xml:lang="de">Taschenpartitur</skos:altLabel>
      <skos:altLabel xml:lang="de">kleine Partitur</skos:altLabel>
      <skos:altLabel xml:lang="it">partiturina</skos:altLabel>
      <skos:altLabel xml:lang="it">partitura tascabile</skos:altLabel>
      <skos:altLabel xml:lang="es">partitura de bolsillo</skos:altLabel>
      <skos:altLabel xml:lang="hu">Zsebpartitúra</skos:altLabel>
      <skos:altLabel xml:lang="ru">карманная партитура</skos:altLabel>
      <skos:altLabel xml:lang="ru">karmannaja partitura</skos:altLabel>
    </skos:Concept>
<!-- Concept: score with only homogeneous groups of instruments of the orchestra  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
