      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">score with only homogeneous groups of instruments of the orchestra</skos:prefLabel>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:altLabel xml:lang="fr">parties en accolade</skos:altLabel>
      <skos:altLabel xml:lang="de">Partitino</skos:altLabel>
      <skos:altLabel xml:lang="it">Spartitino</skos:altLabel>
    </skos:Concept>
<!-- Concept: solo part  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#l" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6464"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">solo part</skos:prefLabel>
      <skos:notation xml:lang="en">l</skos:notation>
      <skos:definition xml:lang="en">Score consisting of a part containing only one solo for a voice of a choir</skos:definition>
      <skos:altLabel xml:lang="es">particela (parte) solista</skos:altLabel>
      <skos:altLabel xml:lang="it">Cartina</skos:altLabel>
    </skos:Concept>
<!-- Concept: tablature  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#h" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6460"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">tablature</skos:prefLabel>
      <skos:notation xml:lang="en">h</skos:notation>
      <skos:definition xml:lang="en">Score consisting of a music notation where letters, numbers or graphic symbols indicate the finger position on the instrument or the name of the notes.</skos:definition>
      <skos:altLabel xml:lang="en">finger notation</skos:altLabel>
      <skos:altLabel xml:lang="fr">tablature</skos:altLabel>
      <skos:altLabel xml:lang="de">Tabulatur</skos:altLabel>
      <skos:altLabel xml:lang="es">Cifra</skos:altLabel>
      <skos:altLabel xml:lang="es">tablatura</skos:altLabel>
      <skos:altLabel xml:lang="it">Intavolatura</skos:altLabel>
      <skos:altLabel xml:lang="hu">tabulatúra</skos:altLabel>
      <skos:altLabel xml:lang="ru">табулатура</skos:altLabel>
      <skos:altLabel xml:lang="ru">tabulatura</skos:altLabel>
    </skos:Concept>
<!-- Concept: table book  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/tos#p" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/6466"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/unimarc/terms/tos#"/>
      <skos:prefLabel xml:lang="en">table book</skos:prefLabel>
      <skos:notation xml:lang="en">p</skos:notation>
      <skos:altLabel xml:lang="it">partitura da tavolo</skos:altLabel>
