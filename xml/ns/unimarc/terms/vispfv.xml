<!-- Scheme: UNIMARC: Visual projections, etc.: Presentation format - videorecording -->
    <skos:ConceptScheme rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#">
        <dc:title>UNIMARC: Visual projections, etc.: Presentation format - videorecording</dc:title>
    </skos:ConceptScheme>

<!-- Concept: Beta (videocassette)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7815"/>
      <skos:prefLabel xml:lang="en">Beta (videocassette)</skos:prefLabel>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:definition xml:lang="en">Home video format introduced in 1975 by Sony Corporation.</skos:definition>
    </skos:Concept>
<!-- Concept: CED (capacitance electronic disk) videodisc  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#h" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7822"/>
      <skos:prefLabel xml:lang="en">CED (capacitance electronic disk) videodisc</skos:prefLabel>
      <skos:notation xml:lang="en">h</skos:notation>
      <skos:definition xml:lang="en">A grooved, plastic disk, usually 12&quot; in diameter, containing information recorded as deep pits in the bottom of the grooves, and played by an electronic stylus. In 1984 RCA, the manufacturer of CED videodisc players, announced its decision to cease production.</skos:definition>
    </skos:Concept>
<!-- Concept: DVD-Video  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#k" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7825"/>
      <skos:prefLabel xml:lang="en">DVD-Video</skos:prefLabel>
      <skos:notation xml:lang="en">k</skos:notation>
      <skos:definition xml:lang="en">Digital Versatile Disc.</skos:definition>
    </skos:Concept>
<!-- Concept: EIAJ (reel)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#d" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7818"/>
      <skos:prefLabel xml:lang="en">EIAJ (reel)</skos:prefLabel>
      <skos:notation xml:lang="en">d</skos:notation>
      <skos:definition xml:lang="en">The standard ½-inch reel-to-reel helical scan videotape format. Named for the Japanese standards committee (the Electronics Industries Association of Japan) which set the standard for ½ inch video tape recorders manufactured since 1969.</skos:definition>
    </skos:Concept>
<!-- Concept: Laser optical (Reflective) videodisc  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#g" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7821"/>
      <skos:prefLabel xml:lang="en">Laser optical (Reflective) videodisc</skos:prefLabel>
      <skos:notation xml:lang="en">g</skos:notation>
      <skos:definition xml:lang="en">A grooveless, smooth round plastic disc with a mirror-like surface, usually 12&quot; in diameter, on both sides of which is stored video information. The disk is read by a weak laser beam. This system became available to the public in 1978 and is currently used in home and industrial applications.</skos:definition>
    </skos:Concept>
<!-- Concept: not a videorecording  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#x" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7827"/>
      <skos:prefLabel xml:lang="en">not a videorecording</skos:prefLabel>
      <skos:notation xml:lang="en">x</skos:notation>
    </skos:Concept>
<!-- Concept: other  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7828"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:notation xml:lang="en">z</skos:notation>
    </skos:Concept>
<!-- Concept: Quadruplex (reel)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#f" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7820"/>
      <skos:prefLabel xml:lang="en">Quadruplex (reel)</skos:prefLabel>
      <skos:notation xml:lang="en">f</skos:notation>
      <skos:definition xml:lang="en">Video system using four recording heads, developed by Ampex in the 1950s.</skos:definition>
    </skos:Concept>
<!-- Concept: Type C (reel)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#e" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7819"/>
      <skos:prefLabel xml:lang="en">Type C (reel)</skos:prefLabel>
      <skos:notation xml:lang="en">e</skos:notation>
      <skos:definition xml:lang="en">Video format using one recording head,  with tape speed at 9.61 inches per second.  This is the broadcast standard in most countries. Type C equipment and technology is made by Sony, RCA, and others.</skos:definition>
    </skos:Concept>
<!-- Concept: U-matic (videocassette)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7817"/>
      <skos:prefLabel xml:lang="en">U-matic (videocassette)</skos:prefLabel>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:definition xml:lang="en">Video format using Sony's trademark name, referring to its tape threading path in a U-shape. Also known as &quot;U type standard&quot;.</skos:definition>
    </skos:Concept>
<!-- Concept: unknown  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7826"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:notation xml:lang="en">u</skos:notation>
    </skos:Concept>
<!-- Concept: V2000 (videocassette)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#i" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7823"/>
      <skos:prefLabel xml:lang="en">V2000 (videocassette)</skos:prefLabel>
      <skos:notation xml:lang="en">i</skos:notation>
      <skos:definition xml:lang="en">Home video format developed in Europe by Philips since 1980. Reversible.</skos:definition>
    </skos:Concept>
<!-- Concept: VHS (videocassette)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7816"/>
      <skos:prefLabel xml:lang="en">VHS (videocassette)</skos:prefLabel>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:definition xml:lang="en">Home video format developed by Japan Victor Corporation (JVC) and marketed in 1977.</skos:definition>
    </skos:Concept>
<!-- Concept: Video8 (videocassette)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/unimarc/terms/vispfv#j" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/unimarc/terms/vispfv#"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/7824"/>
      <skos:prefLabel xml:lang="en">Video8 (videocassette)</skos:prefLabel>
      <skos:notation xml:lang="en">j</skos:notation>
      <skos:definition xml:lang="en">Home video format.</skos:definition>
    </skos:Concept>

