<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:reg="http://metadataregistry.org/uri/profile/regap/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: ISBD Content Form Qualified Base -->
    <skos:ConceptScheme rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase">
        <dc:title>ISBD Content Form Qualified Base</dc:title>
    </skos:ConceptScheme>

<!-- Concept: image (moving ; 2-dimensional)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8679"/>
      <skos:prefLabel xml:lang="en">image (moving ; 2-dimensional)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through line, shape, shading, etc. that is perceived to be in motion, usually by means of a quick succession of images, and in two dimensions.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1002"/>
    </skos:Concept>
<!-- Concept: image (moving ; 3-dimensional)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8680"/>
      <skos:prefLabel xml:lang="en">image (moving ; 3-dimensional)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through line, shape, shading, etc. that is perceived to be in motion, usually by means of a quick succession of images, and in three dimensions.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1002"/>
    </skos:Concept>
<!-- Concept: image (still ; 2-dimensional ; tactile)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1009" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8686"/>
      <skos:prefLabel xml:lang="en">image (still ; 2-dimensional ; tactile)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through line, shape, shading, etc. that is perceived to be static and in two dimensions, and intended to be perceived through touch.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1002"/>
    </skos:Concept>
<!-- Concept: image (still ; 2-dimensional ; visual)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8681"/>
      <skos:prefLabel xml:lang="en">image (still ; 2-dimensional ; visual)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through line, shape, shading, etc. that is perceived to be static and in two dimensions, and intended to be perceived through sight.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1002"/>
    </skos:Concept>
<!-- Concept: image (still ; 3-dimensional)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8682"/>
      <skos:prefLabel xml:lang="en">image (still ; 3-dimensional)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through line, shape, shading, etc. that is perceived to be static and in three dimensions.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1002"/>
    </skos:Concept>
<!-- Concept: movement (notated ; tactile)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1013" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8690"/>
      <skos:prefLabel xml:lang="en">movement (notated ; tactile)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through motion, i.e. the act or process of changing the position of an object or person, and through a notational system for artistic purposes intended to be perceived through touch.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1003"/>
    </skos:Concept>
<!-- Concept: movement (notated)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1008" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8685"/>
      <skos:prefLabel xml:lang="en">movement (notated)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through motion, i.e. the act or process of changing the position of an object or person, and through a notational system for artistic purposes intended to be perceived visually.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1003"/>
    </skos:Concept>
<!-- Concept: music (notated ; tactile)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1012" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8689"/>
      <skos:prefLabel xml:lang="en">music (notated ; tactile)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through ordered tones or sounds in succession, in combination, and in temporal relationships to produce a composition, and through a notational system for artistic purposes intended to be perceived through touch.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1004"/>
    </skos:Concept>
<!-- Concept: music (notated)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1007" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8684"/>
      <skos:prefLabel xml:lang="en">music (notated)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through ordered tones or sounds in succession, in combination, and in temporal relationships to produce a composition, and through a notational system for artistic purposes intended to be perceived visually.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1004"/>
    </skos:Concept>
<!-- Concept: music (performed)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8678"/>
      <skos:prefLabel xml:lang="en">music (performed)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through ordered tones or sounds in succession, in combination, and in temporal relationships to produce a composition, expressed in an audible form, at a given time, and recorded in a resource.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1004"/>
    </skos:Concept>
<!-- Concept: object (tactile)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1010" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8687"/>
      <skos:prefLabel xml:lang="en">object (tactile)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through three-dimensional material, either naturally occurring entities, or human-made/machine-manufactured artefacts, and intended to be perceived through touch.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1005"/>
    </skos:Concept>
<!-- Concept: text (tactile)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1011" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8688"/>
      <skos:prefLabel xml:lang="en">text (tactile)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through written words, symbols and numbers, and intended to be perceived through touch.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1009"/>
    </skos:Concept>
<!-- Concept: text (visual)  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentformbase/T1006" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentformbase"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8683"/>
      <skos:prefLabel xml:lang="en">text (visual)</skos:prefLabel>
      <skos:definition xml:lang="en">Content expressed through written words, symbols and numbers, and intended to be perceived through sight.</skos:definition>
	    <skos:broadMatch rdf:resource="http://iflastandards.info/ns/isbd/terms/contentform/T1009"/>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>
