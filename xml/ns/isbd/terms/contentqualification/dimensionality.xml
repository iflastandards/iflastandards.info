<?xml version="1.0" encoding = "UTF-8"?>
<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:reg="http://metadataregistry.org/uri/profile/regap/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: ISBD Content Qualification of Dimensionality -->
    <skos:ConceptScheme rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality">
        <dc:title>ISBD Content Qualification of Dimensionality</dc:title>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality/T1001"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality/T1002"/>
    </skos:ConceptScheme>

<!-- Concept: 2-dimensional  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1225"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality"/>
      <skos:prefLabel xml:lang="en">2-dimensional</skos:prefLabel>
      <skos:definition xml:lang="en">Image content that is intended to be perceived in two dimensions.</skos:definition>
      <skos:prefLabel xml:lang="es">bidimensional</skos:prefLabel>
      <skos:definition xml:lang="es">Contenido de imagen para que se perciba en dos dimensiones.</skos:definition>
      <skos:prefLabel xml:lang="hr">dvodimenzionalna</skos:prefLabel>
      <skos:prefLabel xml:lang="it">bidimensionale</skos:prefLabel>
      <skos:definition xml:lang="it">Il contenuto di un’immagine che si intende far percepire in due dimensioni.</skos:definition>
      <skos:prefLabel xml:lang="bg">двуизмерно</skos:prefLabel>
      <skos:definition xml:lang="bg">съдържание на изображение, предназначено да се възприема в две измерения.</skos:definition>
      <skos:prefLabel xml:lang="ru">двухмерное</skos:prefLabel>
      <skos:definition xml:lang="ru">Изображение, содержание которого предназначено для восприятия в двух измерениях.</skos:definition>
      <skos:prefLabel xml:lang="sr">dvodimenzionalni</skos:prefLabel>
      <skos:definition xml:lang="sr">Slikovni sadržaj koji treba da se doživi u dve dimenzije.</skos:definition>
      <skos:prefLabel xml:lang="zh">2维</skos:prefLabel>
      <skos:definition xml:lang="zh">设计以二维被感知的图像内容。</skos:definition>
      <skos:prefLabel xml:lang="lv">divdimensiju</skos:prefLabel>
      <skos:definition xml:lang="lv">Attēla saturs, uztverams divās dimensijās.</skos:definition>
      <skos:prefLabel xml:lang="fr">bidimensionnelle</skos:prefLabel>
      <skos:definition xml:lang="fr">Contenu image prévu pour être perçu en deux dimensions.</skos:definition>
    </skos:Concept>
<!-- Concept: 3-dimensional  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1226"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/dimensionality"/>
      <skos:prefLabel xml:lang="en">3-dimensional</skos:prefLabel>
      <skos:definition xml:lang="en">Image content that is intended to be perceived in three dimensions.</skos:definition>
      <skos:prefLabel xml:lang="es">tridimensional</skos:prefLabel>
      <skos:definition xml:lang="es">Contenido de imagen para que se perciba en tres dimensiones.</skos:definition>
      <skos:prefLabel xml:lang="hr">trodimenzionalna</skos:prefLabel>
      <skos:prefLabel xml:lang="it">tridimensionale</skos:prefLabel>
      <skos:definition xml:lang="it">Il contenuto di un’immagine che si intende far percepire in tre dimensioni.</skos:definition>
      <skos:prefLabel xml:lang="bg">триизмерно</skos:prefLabel>
      <skos:definition xml:lang="bg">съдържание на изображение, предназначено да се възприема в три измерения.</skos:definition>
      <skos:prefLabel xml:lang="ru">трехмерное</skos:prefLabel>
      <skos:definition xml:lang="ru">Изображение, содержание которого предназначено для восприятия в трех измерениях.</skos:definition>
      <skos:prefLabel xml:lang="sr">trodimenzionalni</skos:prefLabel>
      <skos:definition xml:lang="sr">Slikovni sadržaj koji treba da se doživi u tri dimenzije.</skos:definition>
      <skos:prefLabel xml:lang="zh">3维</skos:prefLabel>
      <skos:definition xml:lang="zh">设计以三维被感知的图像内容。</skos:definition>
      <skos:prefLabel xml:lang="lv">trīsdimensiju</skos:prefLabel>
      <skos:definition xml:lang="lv">Attēla saturs, uztverams trīs dimensijās.</skos:definition>
      <skos:prefLabel xml:lang="fr">tridimensionnelle</skos:prefLabel>
      <skos:definition xml:lang="fr">Contenu image prévu pour être perçu en trois dimensions.</skos:definition>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
    </skos:Concept>

</rdf:RDF>
