<!-- Scheme: ISBD Content Qualification of Motion -->
    <skos:ConceptScheme rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/motion">
        <dc:title>ISBD Content Qualification of Motion</dc:title>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/motion/T1001"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/motion/T1002"/>
    </skos:ConceptScheme>

<!-- Concept: moving  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/motion/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/motion"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1223"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/motion"/>
      <skos:prefLabel xml:lang="en">moving</skos:prefLabel>
      <skos:definition xml:lang="en">Image content that is perceived to be in motion, usually by means of a quick succession of images.</skos:definition>
      <skos:prefLabel xml:lang="es">en movimiento</skos:prefLabel>
      <skos:definition xml:lang="es">Contenido de imagen que se percibe en movimiento, generalmente por medio de una sucesión rápida de imágenes.</skos:definition>
      <skos:prefLabel xml:lang="hr">pokretna</skos:prefLabel>
      <skos:prefLabel xml:lang="it">in movimento</skos:prefLabel>
      <skos:definition xml:lang="it">Il contenuto di un’immagine che è percepita come in moto, di solito per una rapida successione di immagini.</skos:definition>
      <skos:prefLabel xml:lang="bg">движещо се</skos:prefLabel>
      <skos:definition xml:lang="bg">изображение, чието съдържание се възприема като движещо се, обикновено чрез бърза смяна на изображения.</skos:definition>
      <skos:prefLabel xml:lang="ru">движущееся</skos:prefLabel>
      <skos:definition xml:lang="ru">Изображение, содержание которого воспринимается в движении, обычно посредством быстрой смены изображений.</skos:definition>
      <skos:prefLabel xml:lang="sr">pokretni</skos:prefLabel>
      <skos:definition xml:lang="sr">Slikovni sadržaj koji treba da omogući doživljaj pokreta, obično brzim nizanjem slika jedne za drugom.</skos:definition>
      <skos:prefLabel xml:lang="zh">运动</skos:prefLabel>
      <skos:definition xml:lang="zh">被感觉到运动的图像内容，通常是以前后相继一系列图像的方式。</skos:definition>
      <skos:prefLabel xml:lang="lv">kustīgs</skos:prefLabel>
      <skos:definition xml:lang="lv">Attēla saturs, uztverams kustībā, parasti ar ātru attēlu
nomaiņu.</skos:definition>
      <skos:prefLabel xml:lang="fr">animée</skos:prefLabel>
      <skos:definition xml:lang="fr">Contenu image perçu comme étant en mouvement, habituellement rendu au moyen d’une succession rapide d’images.</skos:definition>
    </skos:Concept>
<!-- Concept: still  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/motion/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/motion"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1224"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/motion"/>
      <skos:prefLabel xml:lang="en">still</skos:prefLabel>
      <skos:prefLabel xml:lang="en">Image content that is perceived to be static.</skos:prefLabel>
      <skos:prefLabel xml:lang="es">fija</skos:prefLabel>
      <skos:definition xml:lang="es">Contenido de imagen que se percibe como estática.</skos:definition>
      <skos:prefLabel xml:lang="hr">nepokretna</skos:prefLabel>
      <skos:prefLabel xml:lang="it">fissa</skos:prefLabel>
      <skos:definition xml:lang="it">Il contenuto di un’immagine che è percepita come statica.</skos:definition>
      <skos:prefLabel xml:lang="bg">неподвижно</skos:prefLabel>
      <skos:definition xml:lang="bg">изображение, чието съдържание се възприема като статично.</skos:definition>
      <skos:prefLabel xml:lang="ru">неподвижное</skos:prefLabel>
      <skos:definition xml:lang="ru">Изображение, содержание которого воспринимается как статическое.</skos:definition>
      <skos:prefLabel xml:lang="sr">nepokretni</skos:prefLabel>
      <skos:definition xml:lang="sr">Slikovni sadržaj koji treba da se doživi kao statičan.</skos:definition>
      <skos:prefLabel xml:lang="zh">静止</skos:prefLabel>
      <skos:definition xml:lang="zh">被感觉到静止的图像内容。</skos:definition>
      <skos:prefLabel xml:lang="lv">nekustīgs</skos:prefLabel>
      <skos:definition xml:lang="lv">Attēla saturs, uztverams kā statisks.</skos:definition>
      <skos:definition xml:lang="en">Image content that is perceived to be static.</skos:definition>
      <skos:prefLabel xml:lang="fr">fixe</skos:prefLabel>
      <skos:definition xml:lang="fr">Contenu image perçu comme étant statique.</skos:definition>
    </skos:Concept>

<!-- Status properties used in this document  -->
    <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001">
        <skos:prefLabel xml:lang="en">Published</skos:prefLabel>
