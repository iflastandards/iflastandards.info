        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication/T1005"/>
    </skos:ConceptScheme>

<!-- Concept: aural  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication/T1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1227"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <skos:prefLabel xml:lang="en">aural</skos:prefLabel>
      <skos:definition xml:lang="en">Content that is intended to be perceived through hearing.</skos:definition>
      <skos:definition xml:lang="es">Contenido para que se perciba a través de la audición.</skos:definition>
      <skos:altLabel xml:lang="es">auditivo</skos:altLabel>
      <skos:altLabel xml:lang="es">auditiva</skos:altLabel>
      <skos:altLabel xml:lang="hr">slušni</skos:altLabel>
      <skos:altLabel xml:lang="hr">slušna</skos:altLabel>
      <skos:prefLabel xml:lang="it">uditivo</skos:prefLabel>
      <skos:definition xml:lang="it">Un contenuto che è destinato ad essere percepito tramite l’udito.</skos:definition>
      <skos:prefLabel xml:lang="bg">слухов</skos:prefLabel>
      <skos:definition xml:lang="bg">съдържание, предназначено да се възприема със слух.</skos:definition>
      <skos:prefLabel xml:lang="ru">слуховой</skos:prefLabel>
      <skos:definition xml:lang="ru">Содержание, предназначенное для восприятия с помощью слуха.</skos:definition>
      <skos:prefLabel xml:lang="zh">听觉</skos:prefLabel>
      <skos:definition xml:lang="zh">设计通过听力被感知的内容。</skos:definition>
      <skos:prefLabel xml:lang="lv">dzirdes</skos:prefLabel>
      <skos:definition xml:lang="lv">Ar dzirdi uztverams saturs.</skos:definition>
      <skos:altLabel xml:lang="fr">auditif</skos:altLabel>
      <skos:altLabel xml:lang="fr">auditive</skos:altLabel>
      <skos:definition xml:lang="fr">Contenu prévu pour être perçu par le sens de l’ouïe.</skos:definition>
    </skos:Concept>
<!-- Concept: gustatory  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication/T1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1228"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <skos:prefLabel xml:lang="en">gustatory</skos:prefLabel>
      <skos:definition xml:lang="en">Content that is intended to be perceived through taste.</skos:definition>
      <skos:definition xml:lang="es">Contenido para que se perciba a través del gusto.</skos:definition>
      <skos:altLabel xml:lang="es">gustativo</skos:altLabel>
      <skos:altLabel xml:lang="es">gustativa</skos:altLabel>
      <skos:altLabel xml:lang="hr">gustativni</skos:altLabel>
      <skos:altLabel xml:lang="hr">gustativna</skos:altLabel>
      <skos:prefLabel xml:lang="it">gustativo</skos:prefLabel>
      <skos:definition xml:lang="it">Un contenuto che è destinato ad essere percepito tramite il gusto.</skos:definition>
      <skos:prefLabel xml:lang="bg">вкусов</skos:prefLabel>
      <skos:definition xml:lang="bg">съдържание, предназначено да се възприема с вкус.</skos:definition>
      <skos:prefLabel xml:lang="ru">вкусовой</skos:prefLabel>
      <skos:definition xml:lang="ru">Содержание, предназначенное для восприятия с помощью вкуса.</skos:definition>
      <skos:prefLabel xml:lang="zh">味觉</skos:prefLabel>
      <skos:definition xml:lang="zh">设计通过味道被感知的内容。</skos:definition>
      <skos:prefLabel xml:lang="lv">garšas</skos:prefLabel>
      <skos:definition xml:lang="lv">Ar garšu uztverams saturs.</skos:definition>
      <skos:altLabel xml:lang="fr">gustatif</skos:altLabel>
      <skos:altLabel xml:lang="fr">gustative</skos:altLabel>
      <skos:definition xml:lang="fr">Contenu prévu pour être perçu par le sens du goût.</skos:definition>
    </skos:Concept>
<!-- Concept: olfactory  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication/T1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1229"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <skos:prefLabel xml:lang="en">olfactory</skos:prefLabel>
      <skos:prefLabel xml:lang="en">Content that is intended to be perceived through smell.</skos:prefLabel>
      <skos:definition xml:lang="es">Contenido para que se perciba a través del olfato.</skos:definition>
      <skos:altLabel xml:lang="es">olfativo</skos:altLabel>
      <skos:altLabel xml:lang="es">olfativa</skos:altLabel>
      <skos:altLabel xml:lang="hr">olfaktivni</skos:altLabel>
      <skos:altLabel xml:lang="hr">olfaktivna</skos:altLabel>
      <skos:prefLabel xml:lang="it">olfattivo</skos:prefLabel>
      <skos:definition xml:lang="it">Un contenuto che è destinato ad essere percepito tramite l’olfatto.</skos:definition>
      <skos:prefLabel xml:lang="bg">обонятелен</skos:prefLabel>
      <skos:definition xml:lang="bg">съдържание, предназначено да се възприема с обоняние.</skos:definition>
      <skos:prefLabel xml:lang="ru">обонятельный</skos:prefLabel>
      <skos:definition xml:lang="ru">Содержание, предназначенное для восприятия с помощью обоняния.</skos:definition>
      <skos:prefLabel xml:lang="zh">嗅觉</skos:prefLabel>
      <skos:definition xml:lang="zh">设计通过气味被感知的内容。</skos:definition>
      <skos:prefLabel xml:lang="lv">ožas</skos:prefLabel>
      <skos:definition xml:lang="lv">Ar ožu uztverams saturs.</skos:definition>
      <skos:altLabel xml:lang="fr">olfactif</skos:altLabel>
      <skos:altLabel xml:lang="fr">olfactive</skos:altLabel>
      <skos:definition xml:lang="fr">Contenu prévu pour être perçu par le sens de l’odorat.</skos:definition>
    </skos:Concept>
<!-- Concept: tactile  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication/T1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1230"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <skos:prefLabel xml:lang="en">tactile</skos:prefLabel>
      <skos:definition xml:lang="en">Content that is intended to be perceived through touch.</skos:definition>
      <skos:prefLabel xml:lang="es">táctil</skos:prefLabel>
      <skos:definition xml:lang="es">Contenido para que se perciba a través del tacto.</skos:definition>
      <skos:altLabel xml:lang="hr">taktilni</skos:altLabel>
      <skos:altLabel xml:lang="hr">taktilna</skos:altLabel>
      <skos:prefLabel xml:lang="it">tattile</skos:prefLabel>
      <skos:definition xml:lang="it">Un contenuto che è destinato ad essere percepito tramite il tatto.</skos:definition>
      <skos:prefLabel xml:lang="bg">тактилен</skos:prefLabel>
      <skos:definition xml:lang="bg">съдържание, предназначено да се възприема с осезание.</skos:definition>
      <skos:prefLabel xml:lang="ru">тактильный</skos:prefLabel>
      <skos:definition xml:lang="ru">Содержание, предназначенное для восприятия с помощью осязания.</skos:definition>
      <skos:prefLabel xml:lang="zh">触觉</skos:prefLabel>
      <skos:definition xml:lang="zh">设计通过接触被感知的内容。</skos:definition>
      <skos:prefLabel xml:lang="lv">taustes</skos:prefLabel>
      <skos:definition xml:lang="lv">Ar tausti uztverams saturs.</skos:definition>
      <skos:prefLabel xml:lang="fr">tactile</skos:prefLabel>
      <skos:definition xml:lang="fr">Contenu prévu pour être perçu par le sens du toucher.</skos:definition>
    </skos:Concept>
<!-- Concept: visual  -->
    <skos:Concept rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication/T1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/1231"/>
      <skos:topConceptOf rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication"/>
      <skos:prefLabel xml:lang="en">visual</skos:prefLabel>
      <skos:definition xml:lang="en">Content that is intended to be perceived through sight.</skos:definition>
      <skos:prefLabel xml:lang="es">visual</skos:prefLabel>
      <skos:definition xml:lang="es">Contenido para que se perciba a través de la vista.</skos:definition>
      <skos:altLabel xml:lang="hr">vizualni</skos:altLabel>
      <skos:altLabel xml:lang="hr">vizualna</skos:altLabel>
      <skos:prefLabel xml:lang="it">visivo</skos:prefLabel>
      <skos:definition xml:lang="it">Un contenuto che è destinato ad essere percepito tramite la vista.</skos:definition>
      <skos:prefLabel xml:lang="bg">визуален</skos:prefLabel>
      <skos:definition xml:lang="bg">съдържание, предназначено да се възприема със зрение.</skos:definition>
      <skos:prefLabel xml:lang="ru">визуальный</skos:prefLabel>
      <skos:definition xml:lang="ru">Содержание, предназначенное для восприятия с помощью зрения.</skos:definition>
      <skos:prefLabel xml:lang="zh">视觉</skos:prefLabel>
      <skos:definition xml:lang="zh">设计通过视力被感知的内容。</skos:definition>
      <skos:prefLabel xml:lang="lv">redzes</skos:prefLabel>
      <skos:definition xml:lang="lv">Ar redzi uztverams saturs.</skos:definition>
      <skos:altLabel xml:lang="fr">visuel</skos:altLabel>
      <skos:altLabel xml:lang="fr">visuelle</skos:altLabel>
      <skos:definition xml:lang="fr">Contenu prévu pour être perçu par le sens de la vue.</skos:definition>
    </skos:Concept>

