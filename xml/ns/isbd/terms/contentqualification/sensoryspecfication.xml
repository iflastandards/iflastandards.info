<!-- Scheme: ISBD Content Qualification of Sensory Specification -->
    <skos:ConceptScheme rdf:about="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication">
        <dc:title>ISBD Content Qualification of Sensory Specification</dc:title>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication/T1001"/>
        <skos:hasTopConcept rdf:resource="http://iflastandards.info/ns/isbd/terms/contentqualification/sensoryspecfication/T1002"/>
