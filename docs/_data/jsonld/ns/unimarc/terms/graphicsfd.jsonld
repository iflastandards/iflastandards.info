            "@type": "Concept",
            "api": "http://api.metadataregistry.org/concepts/6349",
            "definition": {
                "en": "Cover forming the binding or outer enclosure of an item, i.e. hardback or paperback book covers, record sleeves, etc."
            },
            "inScheme": "http://iflastandards.info/ns/unimarc/terms/graphicsfd#",
            "notation": {
                "en": "ab"
            },
            "prefLabel": {
                "en": "item cover",
                "pt": "material de protecção",
                "it": "copertina del documento"
