        {
            "@id": "http://iflastandards.info/ns/unimarc/terms/vispfv#c",
            "@type": "Concept",
            "api": "http://api.metadataregistry.org/concepts/7817",
            "definition": {
                "en": "Video format using Sony's trademark name, referring to its tape threading path in a U-shape. Also known as &quot;U type standard&quot;."
            },
            "inScheme": "http://iflastandards.info/ns/unimarc/terms/vispfv#",
            "notation": {
                "en": "c"
            },
            "prefLabel": {
                "en": "U-matic (videocassette)"
            },
            "status": "Published"
        },
        {
            "@id": "http://iflastandards.info/ns/unimarc/terms/vispfv#d",
            "@type": "Concept",
            "api": "http://api.metadataregistry.org/concepts/7818",
            "definition": {
                "en": "The standard ½-inch reel-to-reel helical scan videotape format. Named for the Japanese standards committee (the Electronics Industries Association of Japan) which set the standard for ½ inch video tape recorders manufactured since 1969."
            },
            "inScheme": "http://iflastandards.info/ns/unimarc/terms/vispfv#",
            "notation": {
                "en": "d"
            },
            "prefLabel": {
                "en": "EIAJ (reel)"
            },
            "status": "Published"
        },
        {
            "@id": "http://iflastandards.info/ns/unimarc/terms/vispfv#e",
            "@type": "Concept",
            "api": "http://api.metadataregistry.org/concepts/7819",
            "definition": {
                "en": "Video format using one recording head,  with tape speed at 9.61 inches per second.  This is the broadcast standard in most countries. Type C equipment and technology is made by Sony, RCA, and others."
            },
            "inScheme": "http://iflastandards.info/ns/unimarc/terms/vispfv#",
            "notation": {
                "en": "e"
            },
            "prefLabel": {
                "en": "Type C (reel)"
            },
            "status": "Published"
        },
        {
            "@id": "http://iflastandards.info/ns/unimarc/terms/vispfv#f",
            "@type": "Concept",
            "api": "http://api.metadataregistry.org/concepts/7820",
            "definition": {
                "en": "Video system using four recording heads, developed by Ampex in the 1950s."
            },
            "inScheme": "http://iflastandards.info/ns/unimarc/terms/vispfv#",
            "notation": {
                "en": "f"
            },
            "prefLabel": {
                "en": "Quadruplex (reel)"
            },
            "status": "Published"
        },
        {
            "@id": "http://iflastandards.info/ns/unimarc/terms/vispfv#g",
            "@type": "Concept",
            "api": "http://api.metadataregistry.org/concepts/7821",
            "definition": {
                "en": "A grooveless, smooth round plastic disc with a mirror-like surface, usually 12\" in diameter, on both sides of which is stored video information. The disk is read by a weak laser beam. This system became available to the public in 1978 and is currently used in home and industrial applications."
            },
            "inScheme": "http://iflastandards.info/ns/unimarc/terms/vispfv#",
            "notation": {
                "en": "g"
            },
            "prefLabel": {
                "en": "Laser optical (Reflective) videodisc"
            },
            "status": "Published"
        },
        {
            "@id": "http://iflastandards.info/ns/unimarc/terms/vispfv#h",
            "@type": "Concept",
            "api": "http://api.metadataregistry.org/concepts/7822",
            "definition": {
                "en": "A grooved, plastic disk, usually 12\" in diameter, containing information recorded as deep pits in the bottom of the grooves, and played by an electronic stylus. In 1984 RCA, the manufacturer of CED videodisc players, announced its decision to cease production."
            },
            "inScheme": "http://iflastandards.info/ns/unimarc/terms/vispfv#",
            "notation": {
                "en": "h"
            },
            "prefLabel": {
                "en": "CED (capacitance electronic disk) videodisc"
            },
            "status": "Published"
        },
        {
