    skos:broadMatch <http://iflastandards.info/ns/isbd/terms/contentform/T1002> ;
    skos:definition "Content expressed through line, shape, shading, etc. that is perceived to be in motion, usually by means of a quick succession of images, and in two dimensions."@en ;
    skos:inScheme <http://iflastandards.info/ns/isbd/terms/contentformbase> ;
    skos:prefLabel "image (moving ; 2-dimensional)"@en .

<http://iflastandards.info/ns/isbd/terms/contentformbase/T1003> a skos:Concept ;
