    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    skos:broadMatch <http://iflastandards.info/ns/isbd/terms/contentform/T1004> ;
    skos:definition "Content expressed through ordered tones or sounds in succession, in combination, and in temporal relationships to produce a composition, expressed in an audible form, at a given time, and recorded in a resource."@en ;
    skos:inScheme <http://iflastandards.info/ns/isbd/terms/contentformbase> ;
    skos:prefLabel "music (performed)"@en .

<http://iflastandards.info/ns/isbd/terms/contentformbase/T1002> a skos:Concept ;
